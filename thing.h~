#ifndef THING_H
#define THING_H
#include <QPixmap> 
#include <QGraphicsPixmapItem> 
class GraphicWindow; 

class Thing: public QGraphicsPixmapItem {
public: 
       Thing (QPixmap *pm, int nx, int ny); 
 	virtual void move() = 0;
 	virtual void collide() = 0;  

private : 
 	int x; 
 	int y; 
 	int vX; 
 	int vY; 
protected :
 	QPixmap *pixMap; 
}; 
#endif
