#include "cloud.h" 
#include <QGraphicsPixmapItem> 
#include <QPixmap> 
#include <iostream> 
#include <QtGui/QApplication> 
#include <cstdlib> 
#include <cmath> 
using namespace std; 

Cloud::Cloud(QPixmap * pm, int x, int y) : Thing(pm, x, y) {
 	y_ = y; 
 	x_ = x; 	
} 

void Cloud::move() {
 	y_ = y_;
 	x_ = x_-1;  
 	QGraphicsPixmapItem::setPos(x_, y_); 
} 
