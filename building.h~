#ifndef BUILDING_H
#define BUILDING_H
#include <QGraphicsPixmapItem> 
#include <QPixmap> 
#include "thing.h" 
class Building : public QObject, public Thing {
 	Q_OBJECT
    public:  
 	Building(QPixmap*, int, int, GraphicWindow*); 
 	~Building(); 
  	void collide(); 
 	int getx(); 
 	int gety(); 
    private: 
 	GraphicWindow * gr; 
 	int x_; 
 	int y_; 

    public slots: 
        void move(); 

};  
#endif 

